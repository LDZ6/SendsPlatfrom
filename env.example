# 数据库密码
MYSQL_USER_PASSWORD=platform_pass_2024
MYSQL_BOBING_PASSWORD=platform_pass_2024
MYSQL_YEARBILL_PASSWORD=platform_pass_2024

# Redis密码
REDIS_USER_PASSWORD=platform_redis_2024
REDIS_BOBING_PASSWORD=platform_redis_2024
REDIS_SCHOOL_PASSWORD=platform_redis_2024
REDIS_YEARBILL_PASSWORD=platform_redis_2024

# Etcd密码
ETCD_PASSWORD=platform_etcd_2024

# RabbitMQ密码
RABBITMQ_PASSWORD=platform_mq_2024

# JWT配置
JWT_KEYS={"v1":"your-jwt-secret-v1","v2":"your-jwt-secret-v2"}
JWT_CURRENT_KEY_ID=v2

# 加密配置
CRYPTO_KEY_VERSION=v1

# 服务配置
PLATFORM_SERVER_VERSION=debug
PLATFORM_SERVER_PORT=:8889

# 日志配置
PLATFORM_OBSERVABILITY_LOGGING_LEVEL=info
PLATFORM_OBSERVABILITY_LOGGING_FORMAT=json
PLATFORM_OBSERVABILITY_LOGGING_SAMPLING_ERRORRATE=1.0
PLATFORM_OBSERVABILITY_LOGGING_SAMPLING_NORMALRATE=0.1

# 监控配置
PLATFORM_OBSERVABILITY_METRICS_ENABLED=true
PLATFORM_OBSERVABILITY_METRICS_PATH=/metrics
PLATFORM_OBSERVABILITY_METRICS_PORT=9090

# 链路追踪配置
PLATFORM_OBSERVABILITY_TRACING_ENABLED=true
PLATFORM_OBSERVABILITY_TRACING_SAMPLERATE=0.1
PLATFORM_OBSERVABILITY_TRACING_SERVICENAME=sends-platform

# 微信配置
WX_APP_ID=your_wechat_app_id
WX_APP_SECRET=your_wechat_app_secret

# 学校系统配置
SCHOOL_API_BASE_URL=https://your-school-api.com
SCHOOL_API_TIMEOUT=30s

# 安全配置
SECURITY_RATE_LIMIT_ENABLED=true
SECURITY_RATE_LIMIT_RPS=100
SECURITY_RATE_LIMIT_BURST=200
SECURITY_MAX_REQUEST_SIZE=10485760
SECURITY_REQUEST_TIMEOUT=30s